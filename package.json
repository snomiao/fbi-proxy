{
  "name": "fbi-proxy",
  "module": "ts/cli.ts",
  "type": "module",
  "devDependencies": {
    "@types/bun": "latest",
    "semantic-release": "^22.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@types/minimist": "^1.2.5",
    "caddy-baron": "^3.0.2",
    "get-port": "^7.1.0",
    "hot-memo": "^1.1.1",
    "minimist": "^1.2.8",
    "phpdie": "^1.7.0",
    "rustup": "^1.0.10"
  },
  "scripts": {
    "build-cli": "bun build ts/cli.ts --outdir dist",
    "build": "bun run build:rs && bun run build:ts",
    "build:ts": "bun build ./ts/*.ts --outdir dist --target node --declaration",
    "build:rs": "cd rs && cargo build --release",
    "dev": "bun --hot ts/cli.ts",
    "start": "bun ts/cli.ts"
  },
  "bin": {
    "fbi-proxy": "ts/cli.ts"
  },
  "version": "1.0.0"
}
